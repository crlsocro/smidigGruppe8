<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".Activities.HistoryActivity"
    android:background="@drawable/loginpage">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/blue"
        android:elevation="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/mapText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="KART"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/backIcon"
            android:layout_width="60dp"
            android:layout_height="50dp"
            android:layout_marginStart="3dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/buttontwo" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            app:layout_constraintBottom_toBottomOf="@+id/backIcon"
            app:layout_constraintEnd_toEndOf="@+id/backIcon"
            app:layout_constraintStart_toStartOf="@+id/backIcon"
            app:layout_constraintTop_toTopOf="@+id/backIcon"
            app:srcCompat="@drawable/backarrow" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/red"
        android:elevation="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout">

        <TextView
            android:id="@+id/pageInfoText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="OVERSIKT"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <ScrollView
        android:id="@+id/historyscrollView"
        android:layout_width="0dp"
        android:layout_height="650dp"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="80dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
        tools:itemCount="5">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    <RelativeLayout
        android:id="@+id/scroll1"
        android:layout_width="match_parent"
        android:layout_marginTop="0dp"
        android:layout_height="150dp"
        android:orientation="vertical"
        android:background="@drawable/gronland">

        <ImageView
            android:id="@+id/bannerIcon"
            android:layout_width="250dp"
            android:layout_height="50dp"
            android:layout_marginTop="50dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:textAlignment="center"
            app:srcCompat="@drawable/banner" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="50dp"
            android:fontFamily="@font/retrojump"
            android:text="GrÃ¸nland"
            android:textColor="@color/yellow"
            android:textSize="75sp"
            app:layout_constraintBottom_toBottomOf="@+id/bannerIcon"
            app:layout_constraintEnd_toEndOf="@+id/bannerIcon"
            app:layout_constraintStart_toStartOf="@+id/bannerIcon"
            app:layout_constraintTop_toTopOf="@+id/bannerIcon" />
            <TextView
                android:id="@+id/grade"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="110dp"
                android:layout_marginLeft="350dp"
                android:text="5"
                android:textSize="22sp"
                android:textColor="@color/yellow"/>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/ic_action_pin"
                android:layout_marginTop="110dp"
                android:layout_toRightOf="@id/grade"
                app:tint="@color/yellow"/>
    </RelativeLayout>
        <RelativeLayout
            android:id="@+id/scroll2"
            android:layout_width="match_parent"
            android:layout_below="@id/scroll1"
            android:layout_height="150dp"
            android:background="@drawable/ss"
            android:alpha="0.6">
            <ImageView
                android:id="@+id/bannerIcon2"
                android:layout_width="250dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:textAlignment="center"
                app:srcCompat="@drawable/banner" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="50dp"
                android:fontFamily="@font/retrojump"
                android:text="Oslo Havn"
                android:textColor="@color/yellow"
                android:textSize="75sp"
                app:layout_constraintBottom_toBottomOf="@+id/bannerIcon2"
                app:layout_constraintEnd_toEndOf="@+id/bannerIcon2"
                app:layout_constraintStart_toStartOf="@+id/bannerIcon2"
                app:layout_constraintTop_toTopOf="@+id/bannerIcon2" />
            <TextView
                android:id="@+id/comingSoon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="60dp"
                android:layout_marginLeft="55dp"
                android:fontFamily="@font/retrojump"
                android:text="Kommer Snart"
                android:textColor="@color/red"
                android:textSize="75sp" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/scroll3"
            android:layout_width="match_parent"
            android:layout_below="@id/scroll2"
            android:layout_height="150dp"
            android:alpha="0.6"
            android:background="@drawable/ruins">

            <ImageView
                android:id="@+id/bannerIcon3"
                android:layout_width="250dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
