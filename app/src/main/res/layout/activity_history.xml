                app:srcCompat="@drawable/banner" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="0dp"
                android:fontFamily="@font/retrojump"
                android:text="Akershus Festning"
                android:textColor="@color/yellow"
                android:textSize="60sp"
                app:layout_constraintBottom_toBottomOf="@+id/bannerIcon5"
                app:layout_constraintEnd_toEndOf="@+id/bannerIcon5"
                app:layout_constraintStart_toStartOf="@+id/bannerIcon5"
                app:layout_constraintTop_toTopOf="@+id/bannerIcon5" />
            <TextView
