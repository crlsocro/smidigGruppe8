            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="0dp"
                android:fontFamily="@font/retrojump"
                android:text="Middelaldersparken"
                android:textColor="@color/yellow"
                android:textSize="60sp"
                app:layout_constraintBottom_toBottomOf="@+id/bannerIcon3"
                app:layout_constraintEnd_toEndOf="@+id/bannerIcon3"
                app:layout_constraintStart_toStartOf="@+id/bannerIcon3"
                app:layout_constraintTop_toTopOf="@+id/bannerIcon3" />
            <TextView
                android:id="@+id/comingSoon2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="60dp"
                android:layout_marginLeft="55dp"
                android:fontFamily="@font/retrojump"
                android:text="Kommer Snart"
                android:textColor="@color/red"
                android:textSize="75sp" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/scroll4"
            android:layout_width="match_parent"
            android:layout_below="@id/scroll3"
            android:layout_height="150dp"
            android:alpha="0.6"
            android:background="@drawable/svarted">
            <ImageView
                android:id="@+id/bannerIcon4"
                android:layout_width="250dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
