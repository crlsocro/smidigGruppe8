                app:srcCompat="@drawable/banner" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="50dp"
                android:fontFamily="@font/retrojump"
                android:text="Oslo 1349"
                android:textColor="@color/yellow"
                android:textSize="75sp"
                app:layout_constraintBottom_toBottomOf="@+id/bannerIcon4"
                app:layout_constraintEnd_toEndOf="@+id/bannerIcon4"
                app:layout_constraintStart_toStartOf="@+id/bannerIcon4"
                app:layout_constraintTop_toTopOf="@+id/bannerIcon4" />
            <TextView
